emacs_man() {
    less <<EOF
    ctrl a       I
    ctrl e       A
    ctrl k       D
    alt  d       de
    ctrl u       d0
    ctrl w       db
    ctrl /       ggVG
    ctrl f       l
    ctrl b       h
    alt  f       w
    alt  b       b
    ctrl l       clear
    ctrl d       delete
EOF
}
alias eman="emacs_man"
